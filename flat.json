{
  "name": "GitHelper",
  "version": "0.1.0",
  "description": "Git Helper Commands",
  "author": "Braden Steffaniak",
  "license": "ISC",
  "defaultTarget": "es6",
  "scripts": {
    "test": "node dist/es6/test.js"
  },
  "sources": {
    "main": {
      "source": "src",
      "dependencies": {
        "https://github.com/FlatLang/System": "master",
        "https://github.com/FlatLang/IO": "master",
        "https://github.com/FlatLang/Git": "master",
        "https://github.com/FlatLang/Logger": "master",
        "https://github.com/FlatLang/GitHubClient": "master",
        "https://github.com/FlatLang/Json": "master",
        "https://github.com/FlatLang/FuCli": "master"
      }
    },
    "cmd": {
      "mainClass": "githelper/GitHelper_Command",
      "source": "src",
      "dependencies": {
      },
      "bin": "git-helper"
    },
    "test": {
      "mainClass": "githelper/GitHelper_TestSuite",
      "source": "src/**/*_Test*.flat",
      "dependencies": {
        "https://github.com/FlatLang/Test": "master"
      }
    }
  }
}
